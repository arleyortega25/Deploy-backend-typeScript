    "@types/passport-local": "^1.0.38",
    "concurrently": "^9.2.0",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "wait-on": "^8.0.4"
  }
}
