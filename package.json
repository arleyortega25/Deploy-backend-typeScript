{
  "name": "nodejs-typescript",
  "version": "1.0.0",
  "main": "dist/sever.js",
  "scripts": {
    "test": "jest",
    "start": "tsc && node dist/src/sever.js",
    "dev": "tsc && concurrently \"tsc -w\" \"wait-on dist/sever.js && nodemon --watch dist --exec node dist/sever.js\""
,
    "start:prod": "SET NODE_ENV=production && npm start",
    "typeorm": "typeorm-ts-node-commonjs",
    "mig:gen": "npm run typeorm -- -d ./src/config/data.source.ts migration:generate ./src/migrations/$npm_config_name",
    "mig:run": "npm run typeorm -- migration:run -d ./src/config/data.source.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.25",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
